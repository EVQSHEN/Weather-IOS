{
  "expo": {
    "name": "my-app",
    "slug": "weatherevq",
    "version": "1.0.0",
    "orientation": "portrait",
    "platforms": ["android", "ios", "web"],
    "icon": "./assets/img/icon.png",
    "userInterfaceStyle": "dark",
    "experiments": {
      "tsconfigPaths": true
    },
    "splash": {
      "image": "./assets/img/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000",
      "textColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.my-app",
      "buildNumber": "1.0.0"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/img/adaptive-icon.png",
        "backgroundColor": "#000000"
      },
      "package": "com.evq.weatherevq",
      "versionCode": 1,
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.FOREGROUND_SERVICE"
      ]
    },
    "owner": "EVQ",
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(my-app) to use your location."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "4d93c1e0-e15c-448f-9c10-f376286ed711"
      }
    }
  }
}
